  "db_name": "PostgreSQL",
  "query": "\n        SELECT DISTINCT w.id, w.name, w.owner_id, w.created_at, w.updated_at\n        FROM workspaces w\n        LEFT JOIN workspace_members wm ON w.id = wm.workspace_id AND wm.user_id = $1\n        WHERE w.owner_id = $1 OR wm.user_id = $1\n        ORDER BY w.created_at DESC\n        ",
