{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            f.id, \n            f.workspace_id, \n            f.parent_id, \n            f.author_id, \n            f.file_type as \"file_type: FileType\", \n            f.status as \"status: FileStatus\", \n            f.slug, \n            f.deleted_at, \n            f.created_at, \n            f.updated_at\n        FROM files f\n        INNER JOIN file_links fl ON f.id = fl.source_file_id\n        WHERE fl.target_file_id = $1\n          AND f.deleted_at IS NULL\n        ORDER BY f.slug ASC\n        ",
