{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            wm.workspace_id,\n            wm.user_id,\n            u.email,\n            u.full_name,\n            wm.role_id,\n            r.name as role_name\n        FROM workspace_members wm\n        JOIN users u ON wm.user_id = u.id\n        JOIN roles r ON wm.role_id = r.id\n        WHERE wm.workspace_id = $1 AND wm.user_id = $2\n        ",
